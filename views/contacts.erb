<p class="contact_counter">There are <strong><%= @contacts.count %></strong> contacts</p>
<ul>
  <% @contacts.each do |contact| %>
    <li><a href="/contacts/<%= contact.id %>"><%= contact.first_name %> <%= contact.last_name %> (<%= contact.email -%>)</a><br><a href="/contacts/<%= contact.id %>/edit">Edit</a> | <a href="/contacts/<%= contact.id %>/delete">Delete</a></li>
  <% end %>
</ul>
