<p class="contact_counter">There are <strong><%= @contacts.count %></strong> contacts</p>
<ul>
  <% @contacts.each do |contact| %>
    <li><a href="/contacts/<%= contact.id %>"><%= contact.first_name %> <%= contact.last_name %> (<%= contact.email -%>)</a><br><a href="#">Edit</a> | <a href="#">Delete</a></li>
  <% end %>
</ul>
